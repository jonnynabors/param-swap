{
  "name": "param-swap",
  "displayName": "param-swap",
  "description": "Swap comma-separated function parameters",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.paramSwap"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.paramSwap",
        "title": "Parameter Swap"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.5",
    "ts-jest": "^25.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "jest": "25.1.0"
  }
}
